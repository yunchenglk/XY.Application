@{
    ViewBag.Title = "留言主题详情";
}
<div class="pageheader">
    <h2>
        <i class="fa fa-cogs"></i> 留言主题详情
    </h2>
    <div class="breadcrumbs">
        <ol class="breadcrumb">
            <li>当前位置</li>
            <li class="active">留言主题详情</li>
        </ol>
    </div>
</div>
<div class="main">
    <section class="tile color transparent-black">
        <div class="tile-body">
            <div class="form-horizontal" method="post" action="/General/NewEdit" id="validate">
                <div class="form-group">
                    <label for="fullname" class="col-sm-3 control-label">姓名: </label>
                    <div class="col-sm-9">
                        <p>@Model.Name</p>
                    </div>
                </div>
                <div class="form-group">
                    <label for="fullname" class="col-sm-3 control-label">邮箱: </label>
                    <div class="col-sm-9">
                        <p>@Model.Email</p>
                    </div>
                </div>
                <div class="form-group">
                    <label for="fullname" class="col-sm-3 control-label">联系方式: </label>
                    <div class="col-sm-9">
                        <p>@Model.Phone</p>
                    </div>
                </div>
                <div class="form-group">
                    <label for="fullname" class="col-sm-3 control-label">时间: </label>
                    <div class="col-sm-9">
                        <p>@Model.CreateTime</p>
                    </div>
                </div>
                <div class="form-group">
                    <label for="fullname" class="col-sm-3 control-label">内容: </label>
                    <div class="col-sm-9">
                        <p>@Model.Content</p>
                    </div>
                </div>
                <div class="form-group form-footer">
                    <div class="col-sm-offset-4 col-sm-8">
                        <a href="/Messages/MessagesDetail/@Model.ID" class="btn btn-primary">回复</a>
                        <a  href="javascript:history.go(-1)" class="btn btn-default">返回</a>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>



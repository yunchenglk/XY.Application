    <script type="text/javascript">
        $(function () {
            var local_href = window.location.href;
            local_href = local_href.toLocaleLowerCase().replace("http://", "");
            var hrefArry = local_href.split('/');
            var over_url = "/";
            for (var i = 1; i < hrefArry.length; i++) {
                over_url += hrefArry[i] + "/";
            }
            over_url = over_url.substr(0, over_url.length - 1);
            $(".menu > .dropdown").each(function (i, n) {
                $(n).find("li").each(function (x, y) {
                    if ($(y).find("a").attr("href").toLocaleLowerCase() == over_url) {
                        $(n).addClass("open");
                        $(y).addClass("active");
                    }
                })

            });
        })
        function getUrlParam(name) {
            //构造一个含有目标参数的正则表达式对象
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
            //匹配目标参数
            var r = window.location.search.substr(1).match(reg);
            //返回参数值
            if (r != null) return unescape(r[2]);
            return null;
        }
    </script>
